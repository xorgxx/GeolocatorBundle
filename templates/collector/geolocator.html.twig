{# templates/collector/geolocator.html.twig #}
{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set status = collector.isAsyncEnabled() ? 'green' : 'red' %}
    {% set icon %}
        <span class="sf-toolbar-label">Geo</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Async enabled</b>
            <span>{{ collector.isAsyncEnabled() ? 'Yes' : 'No' }}</span>
        </div>
    {% endset %}
    {{ include('@WebProfiler/Profiler/toolbar_item.html.twig', { link: false, status: status }) }}
{% endblock %}
